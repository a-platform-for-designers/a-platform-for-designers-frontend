    "axios": "^1.5.1",
    "jsdom": "^22.1.0",
    "normalize.css": "^8.0.1",
    "notistack": "^3.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.3",
